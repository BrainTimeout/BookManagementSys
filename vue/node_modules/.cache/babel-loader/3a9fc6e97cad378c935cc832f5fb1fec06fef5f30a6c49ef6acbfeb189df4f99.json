{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"UserJoin\",\n  props: {\n    // 用于控制账户类型是否禁用\n    isAccountTypeDisabled: {\n      type: Boolean,\n      default: true // 默认禁用\n    }\n  },\n  data() {\n    return {\n      userForm: {\n        account: \"\",\n        username: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n        accountType: \"user\",\n        // 默认设置为用户\n        age: null,\n        gender: \"\",\n        address: \"\",\n        phone: \"\",\n        email: \"\",\n        introduce: \"\"\n      }\n    };\n  },\n  methods: {\n    // 二次密码确认的验证方法\n    validateConfirmPassword(rule, value, callback) {\n      if (value !== this.userForm.password) {\n        callback(new Error(\"两次输入的密码不一致\"));\n      } else {\n        callback();\n      }\n    },\n    async handleSubmit() {\n      try {\n        // 验证表单\n        const isValid = await this.$refs.userForm.validate();\n        if (!isValid) return;\n\n        // 提交数据\n        const res = await request.post(\"/user/UserJoin\", this.userForm);\n        if (res.code === \"200\") {\n          this.$message.success(res.msg);\n          this.$router.push({\n            name: \"UserList\"\n          });\n        } else {\n          this.$message.error(res.msg);\n        }\n      } catch (error) {\n        this.$message.error(res.msg);\n      }\n    },\n    handleReset() {\n      this.$refs.userForm.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["request","name","props","isAccountTypeDisabled","type","Boolean","default","data","userForm","account","username","password","confirmPassword","accountType","age","gender","address","phone","email","introduce","methods","validateConfirmPassword","rule","value","callback","Error","handleSubmit","isValid","$refs","validate","res","post","code","$message","success","msg","$router","push","error","handleReset","resetFields"],"sources":["src/views/user/UserJoin.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-join-container\">\r\n    <el-form :model=\"userForm\" ref=\"userForm\" label-width=\"120px\" class=\"user-form\">\r\n      <!-- 账号 -->\r\n      <el-form-item label=\"账号\" prop=\"account\" :rules=\"[{ required: true, message: '请输入账号', trigger: 'blur' }]\">\r\n        <el-input v-model=\"userForm.account\" placeholder=\"请输入账号\"></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- 用户名 -->\r\n      <el-form-item label=\"用户名\" prop=\"username\" :rules=\"[{ required: true, message: '请输入用户名', trigger: 'blur' }]\">\r\n        <el-input v-model=\"userForm.username\" placeholder=\"请输入用户名\"></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- 密码 -->\r\n      <el-form-item label=\"密码\" prop=\"password\" :rules=\"[{ required: true, message: '请输入密码', trigger: 'blur' }]\">\r\n        <el-input v-model=\"userForm.password\" type=\"password\" placeholder=\"请输入密码\"></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- 确认密码 -->\r\n      <el-form-item label=\"确认密码\" prop=\"confirmPassword\" :rules=\"[ { required: true, message: '请确认密码', trigger: 'blur' }, { validator: validateConfirmPassword, trigger: 'blur' } ]\">\r\n        <el-input v-model=\"userForm.confirmPassword\" type=\"password\" placeholder=\"请再次输入密码\"></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- 账户类型 (默认用户且不可更改) -->\r\n      <el-form-item label=\"账户类型\" prop=\"accountType\" :rules=\"[{ required: true, message: '请选择账户类型', trigger: 'change' }]\">\r\n        <el-select v-model=\"userForm.accountType\" placeholder=\"请选择账户类型\" :disabled=\"isAccountTypeDisabled\">\r\n          <el-option label=\"管理员\" value=\"admin\"></el-option>\r\n          <el-option label=\"用户\" value=\"user\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <!-- 其他字段 -->\r\n      <el-form-item label=\"年龄\" prop=\"age\">\r\n        <el-input v-model=\"userForm.age\" type=\"number\" placeholder=\"请输入年龄\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"性别\" prop=\"gender\">\r\n        <el-select v-model=\"userForm.gender\" placeholder=\"请选择性别\">\r\n          <el-option label=\"男\" value=\"male\"></el-option>\r\n          <el-option label=\"女\" value=\"female\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <!-- 提交与重置按钮 -->\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">提交</el-button>\r\n        <el-button @click=\"handleReset\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"UserJoin\",\r\n  props: {\r\n    // 用于控制账户类型是否禁用\r\n    isAccountTypeDisabled: {\r\n      type: Boolean,\r\n      default: true // 默认禁用\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      userForm: {\r\n        account: \"\",\r\n        username: \"\",\r\n        password: \"\",\r\n        confirmPassword: \"\",\r\n        accountType: \"user\", // 默认设置为用户\r\n        age: null,\r\n        gender: \"\",\r\n        address: \"\",\r\n        phone: \"\",\r\n        email: \"\",\r\n        introduce: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    // 二次密码确认的验证方法\r\n    validateConfirmPassword(rule, value, callback) {\r\n      if (value !== this.userForm.password) {\r\n        callback(new Error(\"两次输入的密码不一致\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      try {\r\n        // 验证表单\r\n        const isValid = await this.$refs.userForm.validate();\r\n        if (!isValid) return;\r\n\r\n        // 提交数据\r\n        const res = await request.post(\"/user/UserJoin\", this.userForm);\r\n        if (res.code === \"200\") {\r\n          this.$message.success(res.msg);\r\n          this.$router.push({ name: \"UserList\" });\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(res.msg);\r\n      }\r\n    },\r\n    handleReset() {\r\n      this.$refs.userForm.resetFields();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-join-container {\r\n  width: 500px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.user-form {\r\n  padding: 20px;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.el-button {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAqDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;IACAC,qBAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,eAAA;QACAC,WAAA;QAAA;QACAC,GAAA;QACAC,MAAA;QACAC,OAAA;QACAC,KAAA;QACAC,KAAA;QACAC,SAAA;MACA;IACA;EACA;EACAC,OAAA;IACA;IACAC,wBAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,IAAAD,KAAA,UAAAf,QAAA,CAAAG,QAAA;QACAa,QAAA,KAAAC,KAAA;MACA;QACAD,QAAA;MACA;IACA;IAEA,MAAAE,aAAA;MACA;QACA;QACA,MAAAC,OAAA,cAAAC,KAAA,CAAApB,QAAA,CAAAqB,QAAA;QACA,KAAAF,OAAA;;QAEA;QACA,MAAAG,GAAA,SAAA9B,OAAA,CAAA+B,IAAA,wBAAAvB,QAAA;QACA,IAAAsB,GAAA,CAAAE,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;YAAApC,IAAA;UAAA;QACA;UACA,KAAAgC,QAAA,CAAAK,KAAA,CAAAR,GAAA,CAAAK,GAAA;QACA;MACA,SAAAG,KAAA;QACA,KAAAL,QAAA,CAAAK,KAAA,CAAAR,GAAA,CAAAK,GAAA;MACA;IACA;IACAI,YAAA;MACA,KAAAX,KAAA,CAAApB,QAAA,CAAAgC,WAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}