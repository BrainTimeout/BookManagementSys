n())\n          .then(res => {\n            this.tableData = res;\n            this.filteredData = res; // 初始时，filteredData = tableData\n            this.updatePagedData();  // 更新分页数据\n          });\n    },\n    handleSearch() {\n      // 搜索时触发的操作，重置页码并更新数据\n      this.currentPage = 1; // 每次搜索时，重置到第一页\n      this.filterData();    // 更新过滤后的数据\n      this.updatePagedData(); // 更新分页数据\n    },\n    handlePageChange(page) {\n      // 页码改变时触发的操作\n      this.currentPage = page;\n      this.updatePagedData(); // 更新分页数据\n    },\n    filterData() {\n      // 根据搜索条件过滤数据\n      if (this.searchQuery === '') {\n        this.filteredData = this.tableData; // 没有搜索条件时显示全部数据\n      } else {\n        this.filteredData = this.tableData.filter(item =>\n            item[this.searchField] && item[this.searchField].toString().toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n      }\n    },\n    updatePagedData() {\n      // 更新分页数据\n      const start = (this.currentPage - 1) * this.pageSize;\n      const end = this.currentPage * this.pageSize;\n      this.pagedData = this.filteredData.slice(start, end);\n    }\n  },\n  mounted() {\n    // 在组件挂载完成后加载数据\n    this.load();\n  },\n  computed: {\n    // 分页后的数据\n    pagedData() {\n      const start = (this.currentPage - 1) * this.pageSize;\n      const end = this.currentPage * this.pageSize;\n      return this.filteredData.slice(start, end);\n    }\n  }\n}\n</script>\n\n<style scoped>\n.table-container {\n  padding: 10px;\n}\n\n.search-form {\n  margin-bottom: 15px;\n  display: flex;\n  align-items: center;\n}\n\n.search-input {\n  width: 250px;\n  margin-right: 10px;\n}\n\n.search-select {\n  width: 150px;\n  margin-right: 10px;\n}\n\n.el-table {\n  margin: 10px 0;\n  border-radius: 8px;\n}\n\n.el-table th, .el-table td {\n  padding: 8px;\n}\n\n.pagination {\n  margin-top: 20px;\n  text-align: center;\n}\n\n.no-data {\n  text-align: center;\n  margin-top: 20px;\n  color: #888;\n}\n</style>\n"],"mappings":";;AAqCA;EACAA,IAAA;EAC