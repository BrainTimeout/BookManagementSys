/div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\" v-for=\"(chart, index) in charts\" :key=\"index\">\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h4>{{ chart.title }}</h4>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <canvas :class=\"chart.canvasClass\"></canvas>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-12\">\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h4>项目信息</h4>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <div class=\"table-responsive\">\r\n                    <table class=\"table table-hover\">\r\n                      <thead>\r\n                      <tr>\r\n                        <th>#</th>\r\n                        <th>项目名称</th>\r\n                        <th>开始日期</th>\r\n                        <th>截止日期</th>\r\n                        <th>状态</th>\r\n                        <th>进度</th>\r\n                      </tr>\r\n                      </thead>\r\n                      <tbody>\r\n                      <tr v-for=\"(project, index) in projects\" :key=\"index\">\r\n                        <td>{{ project.id }}</td>\r\n                        <td>{{ project.name }}</td>\r\n                        <td>{{ project.startDate }}</td>\r\n                        <td>{{ project.endDate }}</td>\r\n                        <td>{{ project.status }}</td>\r\n                        <td>{{ project.progress }}%</td>\r\n                      </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        