{"ast":null,"code":"import request from \"@/utils/request\"; // 请根据你的实际请求方式调整\n\nexport default {\n  name: \"borrowList\",\n  data() {\n    return {\n      tableData: [],\n      // 存储借阅数据\n      total: 0,\n      // 数据总数\n      params: {\n        pageNum: 1,\n        pageSize: 6\n      },\n      filters: {\n        name: \"\",\n        account: \"\",\n        username: \"\",\n        dueDate: []\n      },\n      autoSearch: false,\n      // 控制是否开启自动搜索\n      addDialogVisible: false,\n      // 控制添加借阅对话框显示与否\n      editDialogVisible: false,\n      // 控制编辑借阅对话框显示与否\n      newBorrow: {\n        name: \"\",\n        account: \"\",\n        username: \"\",\n        phone: \"\",\n        dueDate: \"\"\n      },\n      editBorrow: {\n        id: null,\n        name: \"\",\n        account: \"\",\n        username: \"\",\n        phone: \"\",\n        dueDate: \"\"\n      }\n    };\n  },\n  methods: {\n    // 加载数据\n    async load() {\n      try {\n        const res = await request.get(\"/Borrow/Page\", {\n          params: {\n            ...this.params,\n            ...this.filters\n          }\n        });\n        if (res.code === \"200\") {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        } else {\n          this.$message.error(\"加载数据失败\");\n        }\n      } catch (error) {\n        this.$message.error(\"加载数据失败\");\n      }\n    },\n    // 处理输入变化并自动搜索\n    handleInput() {\n      if (this.autoSearch) {\n        this.params.pageNum = 1;\n        this.load();\n      }\n    },\n    // 手动搜索按钮点击事件\n    handleSearch() {\n      this.params.pageNum = 1;\n      this.load();\n    },\n    // 重置搜索条件\n    handleReset() {\n      this.filters = {\n        name: \"\",\n        account: \"\",\n        username: \"\",\n        dueDate: []\n      };\n      this.params.pageNum = 1;\n      this.load();\n    },\n    // 分页变化\n    handlePageChange(page) {\n      this.params.pageNum = page;\n      this.load();\n    },\n    // 编辑借阅记录\n    handleEdit(row) {\n      this.editBorrow = {\n        ...row\n      };\n      this.editDialogVisible = true;\n    },\n    // 保存编辑的借阅记录\n    async handleEditBorrow() {\n      const res = await request.post(`/Borrow/Edit/${this.editBorrow.id}`, this.editBorrow);\n      if (res.code === \"200\") {\n        this.$message.success(\"借阅记录已更新\");\n        this.load();\n        this.editDialogVisible = false;\n      } else {\n        this.$message.error(\"更新借阅记录失败\");\n      }\n    },\n    // 删除借阅记录\n    async handleDelete(row) {\n      try {\n        const res = await request.delete(`/Borrow/Delete/${row.id}`);\n        if (res.code === \"200\") {\n          this.$message.success(\"借阅记录已删除\");\n          this.load();\n        } else {\n          this.$message.error(\"删除借阅记录失败\");\n        }\n      } catch (error) {\n        this.$message.error(\"删除借阅记录失败\");\n      }\n    },\n    // 添加借阅记录\n    async handleAddBorrow() {\n      const res = await request.post(\"/Borrow/Add\", this.newBorrow);\n      if (res.code === \"200\") {\n        this.$message.success(\"借阅记录已添加\");\n        this.load();\n        this.addDialogVisible = false;\n      } else {\n        this.$message.error(\"添加借阅记录失败\");\n      }\n    },\n    // 打开添加借阅对话框\n    openAddDialog() {\n      this.newBorrow = {\n        name: \"\",\n        account: \"\",\n        username: \"\",\n        phone: \"\",\n        dueDate: \"\"\n      };\n      this.addDialogVisible = true;\n    }\n  },\n  mounted() {\n    this.load();\n  }\n};","map":{"version":3,"names":["request","name","data","tableData","total","params","pageNum","pageSize","filters","account","username","dueDate","autoSearch","addDialogVisible","editDialogVisible","newBorrow","phone","editBorrow","id","methods","load","res","get","code","list","$message","error","handleInput","handleSearch","handleReset","handlePageChange","page","handleEdit","row","handleEditBorrow","post","success","handleDelete","delete","handleAddBorrow","openAddDialog","mounted"],"sources":["src/views/borrow/BorrowList.vue"],"sourcesContent":["<template>\r\n  <div class=\"borrow-container\">\r\n    <!-- 搜索框 -->\r\n    <el-row class=\"filter-row\" type=\"flex\" align=\"middle\">\r\n      <el-input\r\n          v-model=\"filters.name\"\r\n          placeholder=\"书名\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-input\r\n          v-model=\"filters.account\"\r\n          placeholder=\"借阅账号\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-input\r\n          v-model=\"filters.username\"\r\n          placeholder=\"用户名\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-date-picker\r\n          v-model=\"filters.dueDate\"\r\n          type=\"daterange\"\r\n          format=\"yyyy-MM-dd\"\r\n          placeholder=\"借阅日期\"\r\n          class=\"filter-input\"\r\n          @change=\"handleInput\"\r\n      />\r\n      <el-switch v-model=\"autoSearch\" @change=\"handleSearch\" />\r\n      <span class=\"auto-search-text\">自动搜索</span>\r\n      <el-button type=\"primary\" @click=\"handleSearch\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"handleReset\">重置</el-button>\r\n    </el-row>\r\n\r\n    <!-- 添加借阅 -->\r\n    <div class=\"add-borrow\">\r\n      <span class=\"add-borrow-text\" @click=\"openAddDialog\">+ 添加借阅记录</span>\r\n    </div>\r\n\r\n    <!-- 借阅记录表格 -->\r\n    <el-table\r\n        :data=\"tableData\"\r\n        stripe\r\n        :default-sort=\"{ prop: 'id', order: 'ascending' }\"\r\n        @sort-change=\"handleSortChange\"\r\n        row-key=\"id\"\r\n    >\r\n      <el-table-column type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n            <el-form-item label=\"借阅人\">\r\n              <span>{{ props.row.username || '暂无信息' }}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"联系电话\">\r\n              <span>{{ props.row.phone || '暂无信息' }}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"借阅时间\">\r\n              <span>{{ props.row.createtime || '暂无信息' }}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"更新时间\">\r\n              <span>{{ props.row.updatetime || '暂无信息' }}</span>\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <!-- 借阅信息列 -->\r\n      <el-table-column prop=\"name\" label=\"书名\" />\r\n      <el-table-column prop=\"bookNo\" label=\"标准码\" />\r\n      <el-table-column prop=\"account\" label=\"借阅账号\" />\r\n      <el-table-column prop=\"dueDate\" label=\"到期日期\" />\r\n\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              size=\"small\"\r\n              type=\"success\"\r\n              icon=\"el-icon-edit\"\r\n              @click=\"handleEdit(scope.row)\"\r\n          >编辑</el-button>\r\n          <el-button\r\n              size=\"small\"\r\n              type=\"danger\"\r\n              icon=\"el-icon-delete\"\r\n              @click=\"handleDelete(scope.row)\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        :current-page=\"params.pageNum\"\r\n        :page-size=\"params.pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handlePageChange\"\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        class=\"pagination\"\r\n    />\r\n\r\n    <!-- 添加借阅对话框 -->\r\n    <el-dialog\r\n        title=\"添加借阅记录\"\r\n        :visible.sync=\"addDialogVisible\"\r\n        width=\"50%\"\r\n    >\r\n      <el-form :model=\"newBorrow\" label-width=\"100px\">\r\n        <el-form-item label=\"书名\">\r\n          <el-input v-model=\"newBorrow.name\" placeholder=\"请输入书名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"借阅账号\">\r\n          <el-input v-model=\"newBorrow.account\" placeholder=\"请输入借阅账号\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"借阅人\">\r\n          <el-input v-model=\"newBorrow.username\" placeholder=\"请输入借阅人姓名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"联系电话\">\r\n          <el-input v-model=\"newBorrow.phone\" placeholder=\"请输入联系电话\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"到期日期\">\r\n          <el-date-picker\r\n              v-model=\"newBorrow.dueDate\"\r\n              type=\"datetime\"\r\n              placeholder=\"请选择到期日期\"\r\n              format=\"yyyy-MM-dd HH:mm:ss\"\r\n              style=\"width: 100%\"\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleAddBorrow\">保存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- 编辑借阅对话框 -->\r\n    <el-dialog\r\n        title=\"编辑借阅记录\"\r\n        :visible.sync=\"editDialogVisible\"\r\n        width=\"50%\"\r\n    >\r\n      <el-form :model=\"editBorrow\" label-width=\"100px\">\r\n        <el-form-item label=\"书名\">\r\n          <el-input v-model=\"editBorrow.name\" placeholder=\"请输入书名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"借阅账号\">\r\n          <el-input v-model=\"editBorrow.account\" placeholder=\"请输入借阅账号\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"借阅人\">\r\n          <el-input v-model=\"editBorrow.username\" placeholder=\"请输入借阅人姓名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"联系电话\">\r\n          <el-input v-model=\"editBorrow.phone\" placeholder=\"请输入联系电话\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"到期日期\">\r\n          <el-date-picker\r\n              v-model=\"editBorrow.dueDate\"\r\n              type=\"datetime\"\r\n              placeholder=\"请选择到期日期\"\r\n              format=\"yyyy-MM-dd HH:mm:ss\"\r\n              style=\"width: 100%\"\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleEditBorrow\">保存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport request from \"@/utils/request\"; // 请根据你的实际请求方式调整\r\n\r\nexport default {\r\n  name: \"borrowList\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 存储借阅数据\r\n      total: 0,       // 数据总数\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 6,\r\n      },\r\n      filters: {\r\n        name: \"\",\r\n        account: \"\",\r\n        username: \"\",\r\n        dueDate: [],\r\n      },\r\n      autoSearch: false,  // 控制是否开启自动搜索\r\n      addDialogVisible: false,  // 控制添加借阅对话框显示与否\r\n      editDialogVisible: false,  // 控制编辑借阅对话框显示与否\r\n      newBorrow: {\r\n        name: \"\",\r\n        account: \"\",\r\n        username: \"\",\r\n        phone: \"\",\r\n        dueDate: \"\",\r\n      },\r\n      editBorrow: {\r\n        id: null,\r\n        name: \"\",\r\n        account: \"\",\r\n        username: \"\",\r\n        phone: \"\",\r\n        dueDate: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    // 加载数据\r\n    async load() {\r\n      try {\r\n        const res = await request.get(\"/Borrow/Page\", {\r\n          params: { ...this.params, ...this.filters },\r\n        });\r\n        if (res.code === \"200\") {\r\n          this.tableData = res.data.list;\r\n          this.total = res.data.total;\r\n        } else {\r\n          this.$message.error(\"加载数据失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"加载数据失败\");\r\n      }\r\n    },\r\n\r\n    // 处理输入变化并自动搜索\r\n    handleInput() {\r\n      if (this.autoSearch) {\r\n        this.params.pageNum = 1;\r\n        this.load();\r\n      }\r\n    },\r\n\r\n    // 手动搜索按钮点击事件\r\n    handleSearch() {\r\n      this.params.pageNum = 1;\r\n      this.load();\r\n    },\r\n\r\n    // 重置搜索条件\r\n    handleReset() {\r\n      this.filters = {\r\n        name: \"\",\r\n        account: \"\",\r\n        username: \"\",\r\n        dueDate: [],\r\n      };\r\n      this.params.pageNum = 1;\r\n      this.load();\r\n    },\r\n\r\n    // 分页变化\r\n    handlePageChange(page) {\r\n      this.params.pageNum = page;\r\n      this.load();\r\n    },\r\n\r\n    // 编辑借阅记录\r\n    handleEdit(row) {\r\n      this.editBorrow = { ...row };\r\n      this.editDialogVisible = true;\r\n    },\r\n\r\n    // 保存编辑的借阅记录\r\n    async handleEditBorrow() {\r\n      const res = await request.post(`/Borrow/Edit/${this.editBorrow.id}`, this.editBorrow);\r\n      if (res.code === \"200\") {\r\n        this.$message.success(\"借阅记录已更新\");\r\n        this.load();\r\n        this.editDialogVisible = false;\r\n      } else {\r\n        this.$message.error(\"更新借阅记录失败\");\r\n      }\r\n    },\r\n\r\n    // 删除借阅记录\r\n    async handleDelete(row) {\r\n      try {\r\n        const res = await request.delete(`/Borrow/Delete/${row.id}`);\r\n        if (res.code === \"200\") {\r\n          this.$message.success(\"借阅记录已删除\");\r\n          this.load();\r\n        } else {\r\n          this.$message.error(\"删除借阅记录失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"删除借阅记录失败\");\r\n      }\r\n    },\r\n\r\n    // 添加借阅记录\r\n    async handleAddBorrow() {\r\n      const res = await request.post(\"/Borrow/Add\", this.newBorrow);\r\n      if (res.code === \"200\") {\r\n        this.$message.success(\"借阅记录已添加\");\r\n        this.load();\r\n        this.addDialogVisible = false;\r\n      } else {\r\n        this.$message.error(\"添加借阅记录失败\");\r\n      }\r\n    },\r\n\r\n    // 打开添加借阅对话框\r\n    openAddDialog() {\r\n      this.newBorrow = {\r\n        name: \"\",\r\n        account: \"\",\r\n        username: \"\",\r\n        phone: \"\",\r\n        dueDate: \"\",\r\n      };\r\n      this.addDialogVisible = true;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.load();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.filter-row {\r\n  margin-bottom: 20px;\r\n}\r\n.filter-input {\r\n  width: 180px;\r\n  margin-right: 10px;\r\n}\r\n.add-borrow-text {\r\n  cursor: pointer;\r\n  color: #409EFF;\r\n  font-size: 16px;\r\n}\r\n.auto-search-text {\r\n  margin-left: 10px;\r\n}\r\n.pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"mappings":"AAgLA,OAAAA,OAAA;;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,KAAA;MAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,OAAA;QACAP,IAAA;QACAQ,OAAA;QACAC,QAAA;QACAC,OAAA;MACA;MACAC,UAAA;MAAA;MACAC,gBAAA;MAAA;MACAC,iBAAA;MAAA;MACAC,SAAA;QACAd,IAAA;QACAQ,OAAA;QACAC,QAAA;QACAM,KAAA;QACAL,OAAA;MACA;MACAM,UAAA;QACAC,EAAA;QACAjB,IAAA;QACAQ,OAAA;QACAC,QAAA;QACAM,KAAA;QACAL,OAAA;MACA;IACA;EACA;EACAQ,OAAA;IACA;IACA,MAAAC,KAAA;MACA;QACA,MAAAC,GAAA,SAAArB,OAAA,CAAAsB,GAAA;UACAjB,MAAA;YAAA,QAAAA,MAAA;YAAA,QAAAG;UAAA;QACA;QACA,IAAAa,GAAA,CAAAE,IAAA;UACA,KAAApB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAsB,IAAA;UACA,KAAApB,KAAA,GAAAiB,GAAA,CAAAnB,IAAA,CAAAE,KAAA;QACA;UACA,KAAAqB,QAAA,CAAAC,KAAA;QACA;MACA,SAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;IAEA;IACAC,YAAA;MACA,SAAAf,UAAA;QACA,KAAAP,MAAA,CAAAC,OAAA;QACA,KAAAc,IAAA;MACA;IACA;IAEA;IACAQ,aAAA;MACA,KAAAvB,MAAA,CAAAC,OAAA;MACA,KAAAc,IAAA;IACA;IAEA;IACAS,YAAA;MACA,KAAArB,OAAA;QACAP,IAAA;QACAQ,OAAA;QACAC,QAAA;QACAC,OAAA;MACA;MACA,KAAAN,MAAA,CAAAC,OAAA;MACA,KAAAc,IAAA;IACA;IAEA;IACAU,iBAAAC,IAAA;MACA,KAAA1B,MAAA,CAAAC,OAAA,GAAAyB,IAAA;MACA,KAAAX,IAAA;IACA;IAEA;IACAY,WAAAC,GAAA;MACA,KAAAhB,UAAA;QAAA,GAAAgB;MAAA;MACA,KAAAnB,iBAAA;IACA;IAEA;IACA,MAAAoB,iBAAA;MACA,MAAAb,GAAA,SAAArB,OAAA,CAAAmC,IAAA,sBAAAlB,UAAA,CAAAC,EAAA,SAAAD,UAAA;MACA,IAAAI,GAAA,CAAAE,IAAA;QACA,KAAAE,QAAA,CAAAW,OAAA;QACA,KAAAhB,IAAA;QACA,KAAAN,iBAAA;MACA;QACA,KAAAW,QAAA,CAAAC,KAAA;MACA;IACA;IAEA;IACA,MAAAW,aAAAJ,GAAA;MACA;QACA,MAAAZ,GAAA