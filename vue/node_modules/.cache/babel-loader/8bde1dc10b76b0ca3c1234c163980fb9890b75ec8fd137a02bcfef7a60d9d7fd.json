{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"revertBook\",\n  data() {\n    return {\n      revertForm: {\n        name: \"\",\n        bookNo: \"\",\n        days: 1,\n        score: null,\n        nums: \"\",\n        account: \"\",\n        balance: \"\",\n        password: \"\",\n        phone: \"\",\n        username: \"\"\n      }\n    };\n  },\n  computed: {\n    // 计算所需积分\n    calculatedScore() {\n      return this.revertForm.score * this.revertForm.days;\n    }\n  },\n  methods: {\n    // 根据书籍编号获取书籍信息\n    async fetchBookDetails() {\n      if (!this.revertForm.bookNo) {\n        return;\n      }\n      try {\n        const res = await request.get(`/Book/GetByBookNo/${this.revertForm.bookNo}`);\n        if (res.code === \"200\") {\n          this.revertForm.name = res.data.name || \"\";\n          this.revertForm.score = res.data.score || null;\n          this.revertForm.nums = res.data.nums || 0;\n        } else {\n          this.revertForm.name = \"\";\n          this.revertForm.score = \"\";\n          this.$message.error(\"未找到该书籍\");\n        }\n      } catch (error) {\n        this.revertForm.name = \"\";\n        this.revertForm.score = \"\";\n        this.$message.error(\"未找到该书籍\");\n      }\n    },\n    // 根据用户账号获取用户信息\n    async fetchUserDetails() {\n      if (!this.revertForm.account) {\n        this.$message.error(\"未找到该用户\");\n        return;\n      }\n      this.$message.error(\"未找到该用户\");\n      try {\n        const res = await request.get(`User/GetAuthInfo/${this.revertForm.account}`);\n        if (res.code === \"200\") {\n          this.revertForm.username = res.data.userProfile.username || \"\";\n          this.revertForm.balance = res.data.balance || \"\";\n          this.revertForm.phone = res.data.userProfile.phone || \"\";\n        } else {\n          this.$message.error(\"未找到该用户\");\n        }\n      } catch (error) {\n        this.$message.error(\"获取用户信息失败\");\n      }\n    },\n    // 提交借阅书籍请求\n    async handleSubmit() {\n      try {\n        // 验证表单\n        const isValid = await this.$refs.revertForm.validate();\n        if (!isValid) return;\n\n        // 构建借阅请求数据\n        const revertRequest = {\n          bookNo: this.revertForm.bookNo,\n          account: this.revertForm.account,\n          days: this.revertForm.days\n        };\n\n        // 发送借阅请求\n        const res = await request.post(\"/Borrow/Insert\", revertRequest);\n        if (res.code === \"200\") {\n          this.$message.success(res.msg);\n          this.$refs.revertForm.resetFields();\n        } else {\n          this.$message.error(res.msg);\n        }\n      } catch (error) {\n        this.$message.error(\"借阅失败\");\n      }\n    },\n    // 重置表单\n    handleReset() {\n      this.$refs.revertForm.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","revertForm","bookNo","days","score","nums","account","balance","password","phone","username","computed","calculatedScore","methods","fetchBookDetails","res","get","code","$message","error","fetchUserDetails","userProfile","handleSubmit","isValid","$refs","validate","revertRequest","post","success","msg","resetFields","handleReset"],"sources":["src/views/borrow/RevertBook.vue"],"sourcesContent":["<template>\r\n  <div class=\"revert-book-container\">\r\n    <el-form :model=\"revertForm\" ref=\"revertForm\" label-width=\"120px\" class=\"revert-form\">\r\n\r\n      <!-- 用户账号 -->\r\n      <el-form-item label=\"用户账号\" prop=\"account\" :rules=\"[{ required: true, message: '请输入用户账号', trigger: 'blur' }]\">\r\n        <el-input v-model=\"revertForm.account\" @blur =\"fetchUserDetails\" placeholder=\"请输入用户账号\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户名 -->\r\n      <el-form-item label=\"用户名\" prop=\"username\">\r\n        <el-input v-model=\"revertForm.username\" :disabled=\"true\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户名 -->\r\n      <el-form-item label=\"用户余额\" prop=\"username\">\r\n        <el-input v-model=\"revertForm.balance\" :disabled=\"true\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户电话 -->\r\n      <el-form-item label=\"用户电话\" prop=\"phone\">\r\n        <el-input v-model=\"revertForm.phone\" :disabled=\"true\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 书籍编号 -->\r\n      <el-form-item label=\"书籍编号\" prop=\"bookNo\" :rules=\"[{ required: true, message: '请输入书籍编号', trigger: 'blur' }]\">\r\n        <el-input v-model=\"revertForm.bookNo\" @blur=\"fetchBookDetails\" placeholder=\"请输入书籍编号\" />\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"借阅天数\" prop=\"score\">\r\n        <el-input-number\r\n            v-model=\"revertForm.days\"\r\n            :min=\"0\"\r\n            :max=\"1000\"\r\n            placeholder=\"请输入借阅天数\"\r\n            style=\"width: 100%;\"\r\n        />\r\n      </el-form-item>\r\n\r\n      <!-- 书籍名称 -->\r\n      <el-form-item label=\"书籍名称\" prop=\"name\">\r\n        <el-input v-model=\"revertForm.name\" :disabled=\"true\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 图书积分 -->\r\n      <el-form-item label=\"所需积分\" prop=\"score\">\r\n        <el-input :value=\"calculatedScore\" :disabled=\"true\" type=\"number\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 图书积分 -->\r\n      <el-form-item label=\"当前数量\" prop=\"score\">\r\n        <el-input v-model=\"revertForm.nums\" :disabled=\"true\" type=\"number\" placeholder=\"自动填充\" />\r\n      </el-form-item>\r\n\r\n      <!-- 提交与重置按钮 -->\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">借阅</el-button>\r\n        <el-button @click=\"handleReset\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"revertBook\",\r\n  data() {\r\n    return {\r\n      revertForm: {\r\n        name: \"\",\r\n        bookNo: \"\",\r\n        days: 1,\r\n        score: null,\r\n        nums:\"\",\r\n        account: \"\",\r\n        balance:\"\",\r\n        password:\"\",\r\n        phone: \"\",\r\n        username: \"\"\r\n      }\r\n    };\r\n  },\r\n  computed: {\r\n    // 计算所需积分\r\n    calculatedScore() {\r\n      return this.revertForm.score * this.revertForm.days;\r\n    }\r\n  },\r\n  methods: {\r\n    // 根据书籍编号获取书籍信息\r\n    async fetchBookDetails() {\r\n      if (!this.revertForm.bookNo) {\r\n        return;\r\n      }\r\n      try {\r\n        const res = await request.get(`/Book/GetByBookNo/${this.revertForm.bookNo}`);\r\n        if (res.code === \"200\") {\r\n          this.revertForm.name = res.data.name || \"\";\r\n          this.revertForm.score = res.data.score || null;\r\n          this.revertForm.nums = res.data.nums || 0;\r\n        } else {\r\n          this.revertForm.name = \"\";\r\n          this.revertForm.score = \"\";\r\n          this.$message.error(\"未找到该书籍\");\r\n        }\r\n      } catch (error) {\r\n        this.revertForm.name = \"\";\r\n        this.revertForm.score = \"\";\r\n        this.$message.error(\"未找到该书籍\");\r\n      }\r\n    },\r\n    // 根据用户账号获取用户信息\r\n    async fetchUserDetails() {\r\n      if (!this.revertForm.account){\r\n        this.$message.error(\"未找到该用户\");\r\n        return;\r\n      }\r\n      this.$message.error(\"未找到该用户\");\r\n      try {\r\n        const res = await request.get(`User/GetAuthInfo/${this.revertForm.account}`);\r\n        if (res.code === \"200\") {\r\n          this.revertForm.username = res.data.userProfile.username || \"\";\r\n          this.revertForm.balance = res.data.balance || \"\";\r\n          this.revertForm.phone = res.data.userProfile.phone || \"\";\r\n        } else {\r\n          this.$message.error(\"未找到该用户\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"获取用户信息失败\");\r\n      }\r\n    },\r\n\r\n    // 提交借阅书籍请求\r\n    async handleSubmit() {\r\n      try {\r\n        // 验证表单\r\n        const isValid = await this.$refs.revertForm.validate();\r\n        if (!isValid) return;\r\n\r\n        // 构建借阅请求数据\r\n        const revertRequest = {\r\n          bookNo: this.revertForm.bookNo,\r\n          account: this.revertForm.account,\r\n          days:this.revertForm.days\r\n        };\r\n\r\n        // 发送借阅请求\r\n        const res = await request.post(\"/Borrow/Insert\", revertRequest);\r\n        if (res.code === \"200\") {\r\n          this.$message.success(res.msg);\r\n          this.$refs.revertForm.resetFields();\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"借阅失败\");\r\n      }\r\n    },\r\n\r\n    // 重置表单\r\n    handleReset() {\r\n      this.$refs.revertForm.resetFields();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.revert-book-container {\r\n  width: 500px;  /* 容器宽度设置 */\r\n  margin: 0 auto; /* 居中显示 */\r\n  padding: 20px; /* 内边距 */\r\n}\r\n\r\n.revert-form {\r\n  padding: 20px;\r\n  background-color: #fff;\r\n  border: none;\r\n  box-shadow: none;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.el-button {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAgEA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;QACAF,IAAA;QACAG,MAAA;QACAC,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,OAAA;QACAC,OAAA;QACAC,QAAA;QACAC,KAAA;QACAC,QAAA;MACA;IACA;EACA;EACAC,QAAA;IACA;IACAC,gBAAA;MACA,YAAAX,UAAA,CAAAG,KAAA,QAAAH,UAAA,CAAAE,IAAA;IACA;EACA;EACAU,OAAA;IACA;IACA,MAAAC,iBAAA;MACA,UAAAb,UAAA,CAAAC,MAAA;QACA;MACA;MACA;QACA,MAAAa,GAAA,SAAAjB,OAAA,CAAAkB,GAAA,2BAAAf,UAAA,CAAAC,MAAA;QACA,IAAAa,GAAA,CAAAE,IAAA;UACA,KAAAhB,UAAA,CAAAF,IAAA,GAAAgB,GAAA,CAAAf,IAAA,CAAAD,IAAA;UACA,KAAAE,UAAA,CAAAG,KAAA,GAAAW,GAAA,CAAAf,IAAA,CAAAI,KAAA;UACA,KAAAH,UAAA,CAAAI,IAAA,GAAAU,GAAA,CAAAf,IAAA,CAAAK,IAAA;QACA;UACA,KAAAJ,UAAA,CAAAF,IAAA;UACA,KAAAE,UAAA,CAAAG,KAAA;UACA,KAAAc,QAAA,CAAAC,KAAA;QACA;MACA,SAAAA,KAAA;QACA,KAAAlB,UAAA,CAAAF,IAAA;QACA,KAAAE,UAAA,CA