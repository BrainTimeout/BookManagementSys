{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      msg: {\n        account: '',\n        password: '',\n        usertype: '',\n        captcha: '' // 添加验证码字段\n      },\n      captchaImage: '/assets/images/captcha.png',\n      // 引入验证码图片\n      logo: require('@/assets/images/logo-sidebar.png') // 引入logo图片\n    };\n  },\n  methods: {\n    async handleLogin() {\n      try {\n        // 发送登录请求，传递msg对象作为请求体\n        const res = await request.post('/Auth/Login', this.msg);\n        if (res.code === \"200\") {\n          this.$notify.success(\"登录成功\");\n          if (res.data.usertype === \"admin\") {\n            await this.$router.push({\n              path: '/admin'\n            }); // 管理员跳转到管理页面\n          } else {\n            await this.$router.push({\n              path: '/'\n            }); // 普通用户跳转到首页\n          }\n        } else {\n          this.$notify.error(\"登录失败\");\n        }\n      } catch {\n        this.$message.error(\"登录失败\");\n      }\n    },\n    refreshCaptcha() {\n      // 点击刷新验证码\n      this.captchaImage = `/assets/images/captcha.png?d=${Math.random()}`; // 动态刷新图片\n    }\n  },\n  computed: {\n    wrapperStyle() {\n      return {\n        backgroundImage: `url(${require('@/assets/images/login-bg-3.jpg')})`,\n        // 引入背景图\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        backgroundRepeat: 'no-repeat',\n        height: '100vh' // 背景图覆盖整个屏幕高度\n      };\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","msg","account","password","usertype","captcha","captchaImage","logo","require","methods","handleLogin","res","post","code","$notify","success","$router","push","path","error","$message","refreshCaptcha","Math","random","computed","wrapperStyle","backgroundImage","backgroundSize","backgroundPosition","backgroundRepeat","height"],"sources":["src/views/login/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"lyear-wrapper\" :style=\"wrapperStyle\">\r\n    <div class=\"lyear-login\">\r\n      <div class=\"login-center\">\r\n        <div class=\"login-header text-center\">\r\n          <a class=\"logo\">图书馆管理系统</a>\r\n        </div>\r\n        <el-form :model=\"msg\" label-width=\"80px\" class=\"login-form\">\r\n          <el-form-item label=\"用户名\" prop=\"account\">\r\n            <el-input v-model=\"msg.account\" placeholder=\"请输入用户名\">\r\n              <template #prepend><i class=\"mdi mdi-account\"></i></template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\" prop=\"password\">\r\n            <el-input v-model=\"msg.password\" type=\"password\" placeholder=\"请输入密码\">\r\n              <template #prepend><i class=\"mdi mdi-lock\"></i></template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"验证码\" prop=\"captcha\">\r\n            <el-row type=\"flex\" justify=\"space-between\" align=\"middle\">\r\n              <!-- 验证码输入框 -->\r\n              <el-col :span=\"16\">\r\n                <el-input v-model=\"msg.captcha\" placeholder=\"请输入验证码\"></el-input>\r\n              </el-col>\r\n              <!-- 验证码图片 -->\r\n              <el-col :span=\"7\">\r\n                <img :src=\"captchaImage\" class=\"captcha-image\" @click=\"refreshCaptcha\" title=\"点击刷新\" alt=\"captcha\">\r\n              </el-col>\r\n            </el-row>\r\n          </el-form-item>\r\n          <!-- 角色选择 -->\r\n          <el-form-item label=\"身份\" prop=\"usertype\">\r\n            <el-radio-group v-model=\"msg.usertype\">\r\n              <el-radio label=\"admin\">管理员</el-radio>\r\n              <el-radio label=\"user\">用户</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" class=\"btn btn-block btn-primary\" @click=\"handleLogin\">立即登录</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      msg: {\r\n        account: '',\r\n        password: '',\r\n        usertype: '',\r\n        captcha: '', // 添加验证码字段\r\n      },\r\n      captchaImage: '/assets/images/captcha.png', // 引入验证码图片\r\n      logo: require('@/assets/images/logo-sidebar.png') // 引入logo图片\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n      try {\r\n        // 发送登录请求，传递msg对象作为请求体\r\n        const res = await request.post('/Auth/Login', this.msg);\r\n        if (res.code === \"200\") {\r\n          this.$notify.success(\"登录成功\");\r\n          if (res.data.usertype === \"admin\") {\r\n            await this.$router.push({ path: '/admin' }); // 管理员跳转到管理页面\r\n          } else {\r\n            await this.$router.push({ path: '/' }); // 普通用户跳转到首页\r\n          }\r\n        } else {\r\n          this.$notify.error(\"登录失败\");\r\n        }\r\n      } catch {\r\n        this.$message.error(\"登录失败\");\r\n      }\r\n    },\r\n    refreshCaptcha() {\r\n      // 点击刷新验证码\r\n      this.captchaImage = `/assets/images/captcha.png?d=${Math.random()}`; // 动态刷新图片\r\n    }\r\n  },\r\n  computed: {\r\n    wrapperStyle() {\r\n      return {\r\n        backgroundImage: `url(${require('@/assets/images/login-bg-3.jpg')})`, // 引入背景图\r\n        backgroundSize: 'cover',\r\n        backgroundPosition: 'center',\r\n        backgroundRepeat: 'no-repeat',\r\n        height: '100vh', // 背景图覆盖整个屏幕高度\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.lyear-wrapper {\r\n  position: relative;\r\n}\r\n\r\n.lyear-login {\r\n  display: flex !important;\r\n  min-height: 100vh;\r\n  align-items: center !important;\r\n  justify-content: center !important;\r\n}\r\n\r\n.login-header {\r\n  margin-bottom: 1.5rem !important;\r\n}\r\n\r\n.logo {\r\n  font-size: 32px;\r\n  font-weight: 600;\r\n  color: #2c3e50;\r\n  text-decoration: none;\r\n  letter-spacing: 2px;\r\n  padding: 5px 15px;\r\n  background-color: transparent;\r\n  border-radius: 8px;\r\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.1); /* 添加文字阴影 */\r\n}\r\n\r\n.login-center {\r\n  background-color: rgba(255, 255, 255, 0.45);\r\n  min-width: 29.25rem;\r\n  padding: 2.14286em 3.57143em;\r\n  border-radius: 3px;\r\n  margin: 2.85714em;\r\n}\r\n\r\n::v-deep .login-center .el-form-item label {\r\n  color: black;\r\n}\r\n\r\n.mdi {\r\n  font-size: 15px;\r\n  color: #409EFF;\r\n}\r\n\r\n.login-center .el-input .el-input__inner {\r\n  background-color: rgba(255, 255, 255, .075);\r\n  border-color: rgba(255, 255, 255, .075);\r\n}\r\n\r\n.login-center .el-button {\r\n  width: 100%;\r\n  background-color: #409EFF;\r\n  border-color: #409EFF;\r\n}\r\n\r\n/* 让验证码图片适应输入框高度 */\r\n.captcha-image {\r\n  width: 120px;\r\n  height: 40px;\r\n  margin-top: 4px;\r\n  vertical-align: middle;\r\n}\r\n\r\n.login-center .el-button {\r\n  height: 40px;\r\n}\r\n</style>\r\n"],"mappings":";AA+CA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,GAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,OAAA;MACA;MACAC,YAAA;MAAA;MACAC,IAAA,EAAAC,OAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,YAAA;MACA;QACA;QACA,MAAAC,GAAA,SAAAb,OAAA,CAAAc,IAAA,qBAAAX,GAAA;QACA,IAAAU,GAAA,CAAAE,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;UACA,IAAAJ,GAAA,CAAAX,IAAA,CAAAI,QAAA;YACA,WAAAY,OAAA,CAAAC,IAAA;cAAAC,IAAA;YAAA;UACA;YACA,WAAAF,OAAA,CAAAC,IAAA;cAAAC,IAAA;YAAA;UACA;QACA;UACA,KAAAJ,OAAA,CAAAK,KAAA;QACA;MACA;QACA,KAAAC,QAAA,CAAAD,KAAA;MACA;IACA;IACAE,eAAA;MACA;MACA,KAAAf,YAAA,mCAAAgB,IAAA,CAAAC,MAAA;IACA;EACA;EACAC,QAAA;IACAC,aAAA;MACA;QACAC,eAAA,SAAAlB,OAAA;QAAA;QACAmB,cAAA;QACAC,kBAAA;QACAC,gBAAA;QACAC,MAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}