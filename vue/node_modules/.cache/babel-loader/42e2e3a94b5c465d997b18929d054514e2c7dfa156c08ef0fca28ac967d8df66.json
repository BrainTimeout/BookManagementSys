{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"BorrowBook\",\n  data() {\n    return {\n      borrowForm: {\n        name: \"\",\n        bookNo: \"\",\n        account: \"\",\n        phone: \"\",\n        username: \"\",\n        score: null\n      }\n    };\n  },\n  methods: {\n    // 提交借阅书籍请求\n    async handleSubmit() {\n      try {\n        // 验证表单\n        const isValid = await this.$refs.borrowForm.validate();\n        if (!isValid) return;\n\n        // 构建借阅请求数据\n        const borrowRequest = {\n          bookDetails: {\n            name: this.borrowForm.name,\n            bookNo: this.borrowForm.bookNo,\n            score: this.borrowForm.score\n          },\n          userDetails: {\n            account: this.borrowForm.account,\n            username: this.borrowForm.username,\n            phone: this.borrowForm.phone\n          }\n        };\n\n        // 发送借阅请求\n        const res = await request.post(\"/api/borrow-book\", borrowRequest);\n        if (res.code === \"200\") {\n          this.$message.success(res.msg);\n          this.$refs.borrowForm.resetFields();\n        } else {\n          this.$message.error(res.msg);\n        }\n      } catch (error) {\n        this.$message.error(\"借阅失败\");\n      }\n    },\n    // 重置表单\n    handleReset() {\n      this.$refs.borrowForm.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","borrowForm","bookNo","account","phone","username","score","methods","handleSubmit","isValid","$refs","validate","borrowRequest","bookDetails","userDetails","res","post","code","$message","success","msg","resetFields","error","handleReset"],"sources":["src/views/borrow/BorrowBook.vue"],"sourcesContent":["<template>\r\n  <div class=\"borrow-book-container\">\r\n    <el-form :model=\"borrowForm\" ref=\"borrowForm\" label-width=\"120px\" class=\"borrow-form\">\r\n      <!-- 书籍名称 -->\r\n      <el-form-item label=\"书籍名称\" prop=\"name\" :rules=\"[{ required: true, message: '请输入书籍名称', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.name\" placeholder=\"请输入书籍名称\" />\r\n      </el-form-item>\r\n\r\n      <!-- 书籍编号 -->\r\n      <el-form-item label=\"书籍编号\" prop=\"bookNo\" :rules=\"[{ required: true, message: '请输入书籍编号', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.bookNo\" placeholder=\"请输入书籍编号\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户账号 -->\r\n      <el-form-item label=\"用户账号\" prop=\"account\" :rules=\"[{ required: true, message: '请输入用户账号', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.account\" placeholder=\"请输入用户账号\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户电话 -->\r\n      <el-form-item label=\"用户电话\" prop=\"phone\" :rules=\"[{ required: true, message: '请输入用户电话', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.phone\" placeholder=\"请输入用户电话\" />\r\n      </el-form-item>\r\n\r\n      <!-- 用户名 -->\r\n      <el-form-item label=\"用户名\" prop=\"username\" :rules=\"[{ required: true, message: '请输入用户名', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.username\" placeholder=\"请输入用户名\" />\r\n      </el-form-item>\r\n\r\n      <!-- 图书积分 -->\r\n      <el-form-item label=\"图书积分\" prop=\"score\" :rules=\"[{ required: true, message: '请输入图书积分', trigger: 'blur' }]\">\r\n        <el-input v-model=\"borrowForm.score\" type=\"number\" placeholder=\"请输入图书积分\" />\r\n      </el-form-item>\r\n\r\n      <!-- 提交与重置按钮 -->\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">借阅</el-button>\r\n        <el-button @click=\"handleReset\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"BorrowBook\",\r\n  data() {\r\n    return {\r\n      borrowForm: {\r\n        name: \"\",\r\n        bookNo: \"\",\r\n        account: \"\",\r\n        phone: \"\",\r\n        username: \"\",\r\n        score: null\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    // 提交借阅书籍请求\r\n    async handleSubmit() {\r\n      try {\r\n        // 验证表单\r\n        const isValid = await this.$refs.borrowForm.validate();\r\n        if (!isValid) return;\r\n\r\n        // 构建借阅请求数据\r\n        const borrowRequest = {\r\n          bookDetails: {\r\n            name: this.borrowForm.name,\r\n            bookNo: this.borrowForm.bookNo,\r\n            score: this.borrowForm.score\r\n          },\r\n          userDetails: {\r\n            account: this.borrowForm.account,\r\n            username: this.borrowForm.username,\r\n            phone: this.borrowForm.phone\r\n          }\r\n        };\r\n\r\n        // 发送借阅请求\r\n        const res = await request.post(\"/api/borrow-book\", borrowRequest);\r\n        if (res.code === \"200\") {\r\n          this.$message.success(res.msg);\r\n          this.$refs.borrowForm.resetFields();\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"借阅失败\");\r\n      }\r\n    },\r\n\r\n    // 重置表单\r\n    handleReset() {\r\n      this.$refs.borrowForm.resetFields();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.borrow-book-container {\r\n  width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f5f5f5;\r\n  border-radius: 8px;\r\n  /* 去掉外部容器的边框和阴影 */\r\n  box-shadow: none;\r\n  border: none;\r\n}\r\n\r\n.borrow-form {\r\n  padding: 20px;\r\n  background-color: #ffffff;\r\n  border-radius: 8px;\r\n  /* 去掉阴影和边框 */\r\n  box-shadow: none;\r\n  border: none;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 20px;\r\n  /* 去掉form-item的边框 */\r\n  border: none;\r\n}\r\n\r\n.el-input__inner {\r\n  /* 去掉输入框的边框 */\r\n  border: none;\r\n  box-shadow: none;\r\n}\r\n\r\n.el-button {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AA2CA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;QACAF,IAAA;QACAG,MAAA;QACAC,OAAA;QACAC,KAAA;QACAC,QAAA;QACAC,KAAA;MA