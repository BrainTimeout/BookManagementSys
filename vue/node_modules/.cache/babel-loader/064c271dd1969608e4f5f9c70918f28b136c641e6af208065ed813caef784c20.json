{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"UserManage\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 12\n      },\n      filters: {\n        account: \"\",\n        username: \"\",\n        age: \"\",\n        sex: null,\n        address: \"\",\n        phone: \"\",\n        detail: \"\"\n      },\n      autoSearch: false,\n      isEditMode: false,\n      // 用于判断是编辑还是添加\n      editDialogVisible: false,\n      editForm: {\n        account: \"\",\n        username: \"\",\n        age: \"\",\n        sex: \"\",\n        address: \"\",\n        phone: \"\",\n        email: \"\",\n        introduce: \"\"\n      }\n    };\n  },\n  watch: {\n    \"params.pageNum\"(newPage) {\n      this.load();\n    },\n    \"params.pageSize\"(newSize) {\n      this.load();\n    }\n  },\n  methods: {\n    async load() {\n      try {\n        const searchParams = {\n          ...this.params,\n          ...this.filters\n        };\n        const res = await request.get(\"/user/InfoPage\", {\n          params: searchParams\n        });\n        if (res.code === \"200\") {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        } else {\n          this.$message.error(\"加载数据失败\");\n        }\n      } catch (error) {\n        this.$message.error(\"加载数据失败\");\n      }\n    },\n    async update() {\n      try {\n        const res = await request.get(\"/user/UpdateInfo\", {\n          params: this.editForm\n        });\n        if (res.code === \"200\") {\n          this.$message.success(\"用户信息已更新！\");\n        } else {\n          this.$message.error(\"用户信息更新失败\");\n        }\n      } catch (error) {\n        this.$message.error(\"用户信息更新失败\");\n      }\n    },\n    async insert() {\n      try {\n        const res = await request.get(\"/user/InsertInfo\", {\n          params: this.editForm\n        });\n        if (res.code === \"200\") {\n          this.$message.success(\"用户信息添加成功！\");\n        } else {\n          this.$message.error(\"用户信息添加失败\");\n        }\n      } catch (error) {\n        this.$message.error(\"用户信息添加失败\");\n      }\n    },\n    handleSearch() {\n      this.load();\n    },\n    handleReset() {\n      this.filters = {\n        account: \"\",\n        username: \"\",\n        age: \"\",\n        sex: null,\n        address: \"\",\n        phone: \"\",\n        detail: \"\"\n      };\n      this.params.pageNum = 1;\n      this.load();\n    },\n    handleInput() {\n      if (this.autoSearch) this.load();\n    },\n    handlePageChange(page) {\n      this.params.pageNum = page;\n    },\n    handleRowClick(row) {\n      this.editForm = {\n        ...row\n      };\n      this.isEditMode = true; // 进入编辑模式\n      this.editDialogVisible = true;\n    },\n    saveEdit() {\n      if (this.isEditMode) {\n        this.update();\n      } else {\n        this.insert();\n      }\n      this.editDialogVisible = false;\n      this.load();\n    },\n    openAddUserDialog() {\n      this.editForm = {\n        account: \"\",\n        username: \"\",\n        age: \"\",\n        sex: \"\",\n        address: \"\",\n        phone: \"\",\n        email: \"\",\n        introduce: \"\"\n      };\n      this.isEditMode = false; // 进入添加模式\n      this.editDialogVisible = true;\n    }\n  },\n  created() {\n    this.load();\n  }\n};","map":{"version":3,"names":["request","name","data","tableData","total","params","pageNum","pageSize","filters","account","username","age","sex","address","phone","detail","autoSearch","isEditMode","editDialogVisible","editForm","email","introduce","watch","params.pageNum","newPage","load","params.pageSize","newSize","methods","searchParams","res","get","code","list","$message","error","update","success","insert","handleSearch","handleReset","handleInput","handlePageChange","page","handleRowClick","row","saveEdit","openAddUserDialog","created"],"sources":["src/views/user/UserManage.vue"],"sourcesContent":["<template>\r\n  <div class=\"table-container\">\r\n    <!-- 筛选条件 -->\r\n    <el-row class=\"filter-row\" type=\"flex\" align=\"middle\">\r\n      <el-input v-model=\"filters.account\" placeholder=\"账户\" class=\"filter-input\" @input=\"handleInput\" />\r\n      <el-input v-model=\"filters.username\" placeholder=\"用户名\" class=\"filter-input\" @input=\"handleInput\" />\r\n      <el-input v-model=\"filters.age\" placeholder=\"年龄\" class=\"filter-input\" @input=\"handleInput\" />\r\n      <el-select v-model=\"filters.sex\" placeholder=\"性别\" class=\"filter-input\" @change=\"handleInput\">\r\n        <el-option label=\"男\" value=\"男\" />\r\n        <el-option label=\"女\" value=\"女\" />\r\n      </el-select>\r\n      <el-input v-model=\"filters.address\" placeholder=\"地址\" class=\"filter-input\" @input=\"handleInput\" />\r\n      <el-input v-model=\"filters.phone\" placeholder=\"电话\" class=\"filter-input\" @input=\"handleInput\" />\r\n      <el-input v-model=\"filters.detail\" placeholder=\"描述信息\" class=\"filter-input\" @input=\"handleInput\" />\r\n    </el-row>\r\n\r\n    <!-- 按钮组 -->\r\n    <el-row class=\"button-row\">\r\n      <el-switch v-model=\"autoSearch\" @change=\"handleSearch\" />\r\n      <span class=\"auto-search-text\">自动搜索</span>\r\n      <div class=\"search-buttons\">\r\n        <el-button type=\"primary\" @click=\"handleSearch\" class=\"btn-spacing\">搜索</el-button>\r\n        <el-button type=\"warning\" @click=\"handleReset\" class=\"btn-spacing\">重置</el-button>\r\n      </div>\r\n    </el-row>\r\n\r\n    <!-- 添加用户按钮 -->\r\n    <div class=\"add-user\" @click=\"openAddUserDialog\">+ 添加用户</div>\r\n\r\n    <!-- 表格 -->\r\n    <el-table :data=\"tableData\" stripe @row-click=\"handleRowClick\">\r\n      <el-table-column prop=\"userid\" label=\"用户ID\" align=\"center\" />\r\n      <el-table-column prop=\"account\" label=\"账户\" align=\"center\" />\r\n      <el-table-column prop=\"username\" label=\"用户名\" align=\"center\" />\r\n      <el-table-column prop=\"age\" label=\"年龄\" align=\"center\" />\r\n      <el-table-column prop=\"sex\" label=\"性别\" align=\"center\" />\r\n      <el-table-column prop=\"address\" label=\"地址\" align=\"center\" />\r\n      <el-table-column prop=\"phone\" label=\"电话\" align=\"center\" />\r\n      <el-table-column label=\"更多\">\r\n        <template v-slot=\"scope\">\r\n          <el-dropdown trigger=\"click\">\r\n            <span class=\"el-dropdown-link\" @click.stop>更多</span>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item><strong>邮箱：</strong>{{ scope.row.email || '暂无邮箱' }}</el-dropdown-item>\r\n              <el-dropdown-item><strong>简介：</strong>{{ scope.row.introduce || '暂无简介' }}</el-dropdown-item>\r\n              <el-dropdown-item><strong>创建时间：</strong>{{ scope.row.createtime || '未知' }}</el-dropdown-item>\r\n              <el-dropdown-item><strong>更新时间：</strong>{{ scope.row.updatetime || '未知' }}</el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        :current-page=\"params.pageNum\"\r\n        :page-size=\"params.pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handlePageChange\"\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        class=\"pagination\"\r\n    />\r\n\r\n    <!-- 编辑/添加用户信息对话框 -->\r\n    <el-dialog title=\"编辑/添加用户信息\" :visible.sync=\"editDialogVisible\" width=\"50%\" class=\"edit-dialog\">\r\n      <el-form :model=\"editForm\" label-width=\"100px\">\r\n        <el-form-item label=\"账户\"><el-input v-model=\"editForm.account\" /></el-form-item>\r\n        <el-form-item label=\"用户名\"><el-input v-model=\"editForm.username\" /></el-form-item>\r\n        <el-form-item label=\"年龄\"><el-input v-model=\"editForm.age\" /></el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-select v-model=\"editForm.sex\">\r\n            <el-option label=\"男\" value=\"男\" />\r\n            <el-option label=\"女\" value=\"女\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\"><el-input v-model=\"editForm.address\" /></el-form-item>\r\n        <el-form-item label=\"电话\"><el-input v-model=\"editForm.phone\" /></el-form-item>\r\n        <el-form-item label=\"邮箱\"><el-input v-model=\"editForm.email\" /></el-form-item>\r\n        <el-form-item label=\"简介\"><el-input v-model=\"editForm.introduce\" /></el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveEdit\">保存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"UserManage\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: { pageNum: 1, pageSize: 12 },\r\n      filters: { account: \"\", username: \"\", age: \"\", sex: null, address: \"\", phone: \"\", detail: \"\" },\r\n      autoSearch: false,\r\n      isEditMode: false, // 用于判断是编辑还是添加\r\n      editDialogVisible: false,\r\n      editForm: {\r\n        account: \"\", username: \"\", age: \"\", sex: \"\", address: \"\", phone: \"\", email: \"\", introduce: \"\",\r\n      },\r\n    };\r\n  },\r\n  watch: {\r\n    \"params.pageNum\"(newPage) { this.load(); },\r\n    \"params.pageSize\"(newSize) { this.load(); },\r\n  },\r\n  methods: {\r\n    async load() {\r\n      try {\r\n        const searchParams = { ...this.params, ...this.filters };\r\n        const res = await request.get(\"/user/InfoPage\", { params: searchParams });\r\n        if (res.code === \"200\") {\r\n          this.tableData = res.data.list;\r\n          this.total = res.data.total;\r\n        } else {\r\n          this.$message.error(\"加载数据失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"加载数据失败\");\r\n      }\r\n    },\r\n    async update() {\r\n      try {\r\n        const res = await request.get(\"/user/UpdateInfo\", { params: this.editForm });\r\n        if (res.code === \"200\") {\r\n          this.$message.success(\"用户信息已更新！\");\r\n        } else {\r\n          this.$message.error(\"用户信息更新失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"用户信息更新失败\");\r\n      }\r\n    },\r\n    async insert(){\r\n      try{\r\n        const res = await request.get(\"/user/InsertInfo\", { params: this.editForm });\r\n        if (res.code === \"200\") {\r\n          this.$message.success(\"用户信息添加成功！\");\r\n        } else {\r\n          this.$message.error(\"用户信息添加失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"用户信息添加失败\");\r\n      }\r\n    },\r\n    handleSearch() { this.load(); },\r\n    handleReset() {\r\n      this.filters = { account: \"\", username: \"\", age: \"\", sex: null, address: \"\", phone: \"\", detail: \"\" };\r\n      this.params.pageNum = 1;\r\n      this.load();\r\n    },\r\n    handleInput() { if (this.autoSearch) this.load(); },\r\n    handlePageChange(page) { this.params.pageNum = page; },\r\n    handleRowClick(row) {\r\n      this.editForm = { ...row };\r\n      this.isEditMode = true; // 进入编辑模式\r\n      this.editDialogVisible = true;\r\n    },\r\n    saveEdit() {\r\n      if (this.isEditMode) {\r\n        this.update();\r\n      } else {\r\n        this.insert();\r\n      }\r\n      this.editDialogVisible = false;\r\n      this.load();\r\n    },\r\n    openAddUserDialog() {\r\n      this.editForm = { account: \"\", username: \"\", age: \"\", sex: \"\", address: \"\", phone: \"\", email: \"\", introduce: \"\" };\r\n      this.isEditMode = false; // 进入添加模式\r\n      this.editDialogVisible = true;\r\n    },\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table-container {\r\n  padding: 10px;\r\n}\r\n\r\n.fi