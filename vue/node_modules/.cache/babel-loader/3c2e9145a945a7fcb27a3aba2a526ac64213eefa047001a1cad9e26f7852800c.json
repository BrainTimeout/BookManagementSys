{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      user: {\n        username: '张三',\n        avatar: require(\"@/assets/images/captcha.png\"),\n        email: 'zhangsan@example.com'\n      },\n      logo: require(\"@/assets/images/captcha.png\"),\n      dropdownVisible: false,\n      sidebarVisible: true,\n      userInfoDialogVisible: false,\n      changePasswordDialogVisible: false,\n      newPassword: '',\n      confirmPassword: ''\n    };\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarVisible = !this.sidebarVisible;\n    },\n    toggleDropdown() {\n      this.dropdownVisible = !this.dropdownVisible;\n    },\n    showUserInfoDialog() {\n      this.userInfoDialogVisible = true;\n    },\n    showChangePasswordDialog() {\n      this.changePasswordDialogVisible = true;\n    },\n    changePassword() {\n      if (this.newPassword === this.confirmPassword) {\n        // 在此处理密码更改的逻辑\n        this.$message.success('密码修改成功');\n        this.changePasswordDialogVisible = false;\n      } else {\n        this.$message.error('两次输入的密码不一致');\n      }\n    },\n    logout() {\n      this.$router.push('/login'); // 退出登录，跳转到登录页\n    }\n  }\n};","map":{"version":3,"names":["data","user","username","avatar","require","email","logo","dropdownVisible","sidebarVisible","userInfoDialogVisible","changePasswordDialogVisible","newPassword","confirmPassword","methods","toggleSidebar","toggleDropdown","showUserInfoDialog","showChangePasswordDialog","changePassword","$message","success","error","logout","$router","push"],"sources":["src/views/layout/Admin.vue"],"sourcesContent":["<template>\r\n  <div class=\"layout\">\r\n    <div class=\"left-content\" :class=\"{ visible: sidebarVisible }\">\r\n      <div class=\"sidebar-header\">\r\n        <img :src=\"logo\" alt=\"Logo\" class=\"sidebar-logo\" />\r\n        <span class=\"sidebar-title\">图书馆管理系统</span>\r\n      </div>\r\n      <el-menu :default-active=\"$route.path\" :default-openeds=\"['1','2']\" router class=\"el-menu-demo\">\r\n        <el-menu-item index=\"/\">\r\n          <i class=\"mdi mdi-home\"></i>\r\n          <span>首页</span>\r\n        </el-menu-item>\r\n        <el-submenu index=\"1\">\r\n          <template #title>\r\n            <i class=\"mdi mdi-book-open-page-variant\"></i>\r\n            <span>书籍管理</span>\r\n          </template>\r\n          <el-menu-item index=\"/bookCategoryManage\">\r\n            <i class=\"mdi mdi-bookmark-outline\"></i>\r\n            <span>书籍分类管理</span>\r\n          </el-menu-item>\r\n        </el-submenu>\r\n        <el-submenu index=\"2\">\r\n          <template #title>\r\n            <i class=\"mdi mdi-account-settings\"></i>\r\n            <span>用户管理</span>\r\n          </template>\r\n          <el-menu-item index=\"/userManage\">\r\n            <i class=\"mdi mdi-account-edit\"></i>\r\n            <span>用户信息管理</span>\r\n          </el-menu-item>\r\n          <el-menu-item index=\"/accountManage\">\r\n            <i class=\"mdi mdi-account-cog\"></i>\r\n            <span>用户账号管理</span>\r\n          </el-menu-item>\r\n          <el-menu-item index=\"/userJoin\">\r\n            <i class=\"mdi mdi-account-plus\"></i>\r\n            <span>用户注册</span>\r\n          </el-menu-item>\r\n        </el-submenu>\r\n      </el-menu>\r\n    </div>\r\n    <div class=\"right-content\" :class=\"{ shifted: sidebarVisible }\">\r\n      <header class=\"header\">\r\n        <button @click=\"toggleSidebar\" class=\"sidebar-toggle\">\r\n          <span :class=\"sidebarVisible ? 'mdi mdi-menu-open' : 'mdi mdi-menu'\"></span>\r\n          <span class=\"button-text\">菜单</span>\r\n        </button>\r\n        <div class=\"user-info\" @click=\"toggleDropdown\">\r\n          <img :src=\"user.avatar\" alt=\"Avatar\" class=\"avatar\" />\r\n          <span class=\"username\">{{ user.username }}</span>\r\n          <span :class=\"dropdownVisible ? 'mdi mdi-menu-down' : 'mdi mdi-menu-up'\"></span>\r\n        </div>\r\n      </header>\r\n      <main class=\"main\">\r\n        <router-view/>\r\n        <div v-if=\"dropdownVisible\" class=\"dropdown\">\r\n          <ul>\r\n            <li @click=\"showUserInfoDialog\"><i class=\"mdi mdi-account-circle\"></i> 个人信息</li>\r\n            <li @click=\"showChangePasswordDialog\"><i class=\"mdi mdi-lock\"></i> 修改密码</li>\r\n            <li @click=\"logout\"><i class=\"mdi mdi-logout\"></i> 退出登录</li>\r\n          </ul>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- 个人信息对话框 -->\r\n  <el-dialog title=\"个人信息\" :visible.sync=\"userInfoDialogVisible\" width=\"30%\">\r\n    <div>\r\n      <p>用户名: {{ user.username }}</p>\r\n      <p>邮箱: {{ user.email }}</p>\r\n      <!-- 其他个人信息 -->\r\n    </div>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"userInfoDialogVisible = false\">关闭</el-button>\r\n    </span>\r\n  </el-dialog>\r\n\r\n  <!-- 修改密码对话框 -->\r\n  <el-dialog title=\"修改密码\" :visible.sync=\"changePasswordDialogVisible\" width=\"30%\">\r\n    <div>\r\n      <el-input v-model=\"newPassword\" placeholder=\"请输入新密码\" type=\"password\"></el-input>\r\n      <el-input v-model=\"confirmPassword\" placeholder=\"确认新密码\" type=\"password\"></el-input>\r\n    </div>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"changePasswordDialogVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"changePassword\">确定</el-button>\r\n    </span>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: '张三',\r\n        avatar: require(\"@/assets/images/captcha.png\"),\r\n        email: 'zhangsan@example.com'\r\n      },\r\n      logo: require(\"@/assets/images/captcha.png\"),\r\n      dropdownVisible: false,\r\n      sidebarVisible: true,\r\n      userInfoDialogVisible: false,\r\n      changePasswordDialogVisible: false,\r\n      newPassword: '',\r\n      confirmPassword: ''\r\n    };\r\n  },\r\n  methods: {\r\n    toggleSidebar() {\r\n      this.sidebarVisible = !this.sidebarVisible;\r\n    },\r\n    toggleDropdown() {\r\n      this.dropdownVisible = !this.dropdownVisible;\r\n    },\r\n    showUserInfoDialog() {\r\n      this.userInfoDialogVisible = true;\r\n    },\r\n    showChangePasswordDialog() {\r\n      this.changePasswordDialogVisible = true;\r\n    },\r\n    changePassword() {\r\n      if (this.newPassword === this.confirmPassword) {\r\n        // 在此处理密码更改的逻辑\r\n        this.$message.success('密码修改成功');\r\n        this.changePasswordDialogVisible = false;\r\n      } else {\r\n        this.$message.error('两次输入的密码不一致');\r\n      }\r\n    },\r\n    logout() {\r\n      this.$router.push('/login'); // 退出登录，跳转到登录页\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 保留之前的样式不变 */\r\n.layout { display: flex; height: 100vh; }\r\n.left-content { width: 15%; position: fixed; top: 0; left: 0; background-color: #f4f4f4; height: 100%; transform: translateX(-100%); transition: transform 0.3s; }\r\n.left-content.visible { transform: translateX(0); }\r\n.sidebar-header, .header { display: flex; align-items: center; padding: 0 20px; }\r\n.sidebar-header { height: 10%; border-bottom: 1px solid #ddd; }\r\n.sidebar-logo { width: 30px; height: 30px; border-radius: 50%; margin-right: 10px; }\r\n.sidebar-title { font-size: 18px; font-weight: bold; color: #333; }\r\n.right-content { width: 100%; display: flex; flex-direction: column; transition: margin-left 0.3s ,width 0.3s; }\r\n.right-content.shifted { margin-left: 15%; width: 85%; }\r\n.header { background-color: #e0e0e0; justify-content: flex-start; height: 10%; }\r\n.user-info { display: flex; align-items: center; margin-left: auto; cursor: pointer; }\r\n.avatar { width: 50px; height: 50px; border-radius: 50%; margin-right: 15px; }\r\n.username { margin-right: 10px; font-size: 18px; color: #333; }\r\n.mdi { font-size: 24px; }\r\n.sidebar-toggle { display: flex; align-items: center; background-color: transparent; border: none; font-size: 16px; cursor: pointer; }\r\n.sidebar-toggle .mdi { margin-right: 8px; font-size: 24px; }\r\n.dropdown { position: absolute; right: 10px; top: 10%; background-color: #fff; box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); width: 150px; z-index: 10; }\r\n.dropdown ul { list-style: none; padding: 10px 0; margin: 0; }\r\n.dropdown li { padding: 10px; font-size: 17px; color: #333; cursor: pointer; }\r\n.dropdown li:hover { background-color: #f0f0f0; }\r\n.main { flex-grow: 1; background-color: #fff; }\r\n</style>\r\n"],"mappings":";AA6FA;EACAA,KAAA;IACA;MACAC,IAAA;QACAC,QAAA;QACAC,MAAA,EAAAC,OAAA;QACAC,KAAA;MACA;MACAC,IAAA,EAAAF,OAAA;MACAG,eAAA;MACAC,cAAA;MACAC,qBAAA;MACAC,2BAAA;MACAC,WAAA;MACAC,eAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACA,KAAAN,cAAA,SAAAA,cAAA;IACA;IACAO,eAAA;MACA,KAAAR,eAAA,SAAAA,eAAA;IACA;IACAS,mBAAA;MACA,KAAAP,qBAAA;IACA;IACAQ,yBAAA;MACA,KAAAP,2BAAA;IACA;IACAQ,eAAA;MACA,SAAAP,WAAA,UAAAC,eAAA;QACA;QACA,KAAAO,QAAA,CAAAC,OAAA;QACA,KAAAV,2BAAA;MACA;QACA,KAAAS,QAAA,CAAAE,KAAA;MACA;IACA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}