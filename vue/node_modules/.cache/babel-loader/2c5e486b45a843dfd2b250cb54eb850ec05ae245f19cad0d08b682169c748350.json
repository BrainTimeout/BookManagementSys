{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        captcha: '',\n        rememberMe: false,\n        role: 'user' // 默认选择 \"用户\"\n      },\n      captchaImage: require('@/assets/images/captcha.png'),\n      // 引入验证码图片\n      logo: require('@/assets/images/logo-sidebar.png') // 引入logo图片\n    };\n  },\n  methods: {\n    handleLogin() {\n      console.log(\"登录信息：\", this.form);\n      this.$router.push({\n        path: '/index'\n      }); // 登录成功后重定向\n    },\n    refreshCaptcha() {\n      this.captchaImage = require(`@/assets/images/captcha.png?d=${Math.random()}`);\n    }\n  }\n};","map":{"version":3,"names":["name","data","form","username","password","captcha","rememberMe","role","captchaImage","require","logo","methods","handleLogin","console","log","$router","push","path","refreshCaptcha","Math","random"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"lyear-wrapper\" :style=\"wrapperStyle\">\r\n    <div class=\"lyear-login\">\r\n      <div class=\"login-center\">\r\n        <div class=\"login-header text-center\">\r\n          <a class=\"logo\">图书馆管理系统</a>\r\n        </div>\r\n        <el-form @submit.prevent=\"handleLogin\" :model=\"form\" label-width=\"80px\" class=\"login-form\">\r\n          <el-form-item label=\"用户名\" prop=\"username\">\r\n            <el-input v-model=\"form.username\" placeholder=\"请输入用户名\">\r\n              <template #prepend><i class=\"mdi mdi-account\"></i></template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\" prop=\"password\">\r\n            <el-input v-model=\"form.password\" type=\"password\" placeholder=\"请输入密码\">\r\n              <template #prepend><i class=\"mdi mdi-lock\"></i></template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"验证码\" prop=\"captcha\"