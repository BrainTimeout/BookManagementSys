ad();\n      }\n    },\n    handlePageChange(page) {\n      this.params.pageNum = page;\n    },\n    handleRowClick(row) {\n      this.editForm = {\n        ...row\n      };\n      this.editDialogVisible = true;\n    },\n    saveEdit() {\n      this.update();\n      this.editDialogVisible = false;\n      this.load();\n    }\n  },\n  created() {\n    this.load();\n  }\n};","map":{"version":3,"names":["request","name","data","tableData","total","params","pageNum","pageSize","filters","account","username","bookNo","bookName","dueDate","autoSearch","editDialogVisible","editForm","id","watch","params.pageNum","newPage","load","params.pageSize","newSize","methods","searchParams","res","get","code","list","$message","error","update","put","success","msg","handleSearch","handleReset","handleInput","handlePageChange","page","handleRowClick","row","saveEdit","created"],"sources":["src/views/borrow/BorrowList.vue"],"sourcesContent":["<template>\r\n  <div class=\"table-container\">\r\n    <!-- 筛选条件 -->\r\n    <el-row class=\"filter-row\" type=\"flex\" align=\"middle\">\r\n      <el-input\r\n          v-model=\"filters.account\"\r\n          placeholder=\"账户\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-input\r\n          v-model=\"filters.username\"\r\n          placeholder=\"用户名\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-input\r\n          v-model=\"filters.bookNo\"\r\n          placeholder=\"图书编号\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-input\r\n          v-model=\"filters.bookName\"\r\n          placeholder=\"图书名称\"\r\n          class=\"filter-input\"\r\n          @input=\"handleInput\"\r\n      />\r\n      <el-date-picker\r\n          v-model=\"filters.dueDate\"\r\n          type=\"date\"\r\n          placeholder=\"到期日期\"\r\n          class=\"filter-input\"\r\n          @change=\"handleInput\"\r\n      />\r\n    </el-row>\r\n\r\n    <!-- 按钮组 -->\r\n    <el-row class=\"button-row\">\r\n      <el-switch v-model=\"autoSearch\" @change=\"handleSearch\" />\r\n      <span class=\"auto-search-text\">自动搜索</span>\r\n      <div class=\"search-buttons\">\r\n        <el-button type=\"primary\" @click=\"handleSearch\" class=\"btn-spacing\">搜索</el-button>\r\n        <el-button type=\"warning\" @click=\"handleReset\" class=\"btn-spacing\">重置</el-button>\r\n      </div>\r\n    </el-row>\r\n\r\n    <!-- 表格 -->\r\n    <el-table :data=\"tableData\" stripe @row-click=\"handleRowClick\">\r\n      <el-table-column prop=\"bookName\" label=\"书籍名称\" />\r\n      <el-table-column prop=\"bookNo\" label=\"书籍编号\" />\r\n      <el-table-column prop=\"account\" label=\"账户\" />\r\n      <el-table-column prop=\"username\" label=\"用户名\" />\r\n      <el-table-column prop=\"dueDate\" label=\"到期日期\" />\r\n      <el-table-column prop=\"createtime\" label=\"借阅日期\" />\r\n      <el-table-column prop=\"phone\" label=\"电话\" />\r\n      <el-table-column label=\"更多\">\r\n        <template slot-scope=\"scope\">\r\n          <el-dropdown trigger=\"click\">\r\n            <span class=\"el-dropdown-link\" @click.stop>更多</span>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item>\r\n                <strong>更新日期：</strong>{{ scope.row.updatetime || '未知' }}\r\n              </el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        :current-page=\"params.pageNum\"\r\n        :page-size=\"params.pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handlePageChange\"\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        class=\"pagination\"\r\n    />\r\n\r\n    <!-- 编辑借阅记录对话框 -->\r\n    <el-dialog title=\"编辑借阅记录\" :visible.sync=\"editDialogVisible\" width=\"50%\" class=\"edit-dialog\">\r\n      <el-form :model=\"editForm\" label-width=\"100px\">\r\n        <el-form-item label=\"到期日期\">\r\n          <el-date-picker v-model=\"editForm.dueDate\" type=\"date\" placeholder=\"选择到期日期\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveEdit\">保存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"BorrowList\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 12,\r\n      },\r\n      filters: {\r\n        account: \"\",\r\n        username: \"\",\r\n        bookNo: \"\",\r\n        bookName: \"\",\r\n        dueDate: null,\r\n      },\r\n      autoSearch: false,\r\n      editDialogVisible: false,\r\n      editForm: {\r\n        id: \"\",\r\n        dueDate: \"\",\r\n      },\r\n    };\r\n  },\r\n  watch: {\r\n    \"params.pageNum\"(newPage) {\r\n      this.load();\r\n    },\r\n    \"params.pageSize\"(newSize) {\r\n      this.load();\r\n    },\r\n  },\r\n  methods: {\r\n    async load() {\r\n      try {\r\n        const searchParams = { ...this.params, ...this.filters };\r\n        const res = await request.get(\"/BorrowProfile/Page\", { params: searchParams });\r\n        if (res.code === \"200\") {\r\n          this.tableData = res.data.list;\r\n          this.total = res.data.total;\r\n        } else {\r\n          this.$message.error(\"加载数据失败\");\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"加载数据失败\");\r\n      }\r\n    },\r\n    async update() {\r\n      try {\r\n        const res = await request.put(\"/Borrow/Update\", this.editForm);\r\n        if (res.code === \"200\") {\r\n          this.$message.success(\"借阅记录已更新！\");\r\n          this.load();\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      } catch (error) {\r\n        this.$message.error(\"借阅记录更新失败\");\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.params.pageNum = 1;\r\n      this.load();\r\n    },\r\n    handleReset() {\r\n      this.filters = {\r\n        account: \"\",\r\n        username: \"\",\r\n        bookNo: \"\",\r\n        bookName: \"\",\r\n        dueDate: null,\r\n      };\r\n      this.params.pageNum = 1;\r\n      this.load();\r\n    },\r\n    handleInput() {\r\n      if (this.autoSearch) {\r\n        this.params.pageNum = 1;\r\n        this.load();\r\n      }\r\n    },\r\n    handlePageChange(page) {\r\n      this.params.pageNum = page;\r\n    },\r\n    handleRowClick(row) {\r\n      this.editForm = { ...row };\r\n      this.editDialogVisible = true;\r\n    },\r\n    saveEdit() {\r\n      this.update();\r\n      this.editDialogVisible = false;\r\n      this.load();\r\n    },\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table-container {\r\n  padding: 10px;\r\n}\r\n\r\n.filter-row {\r\n  display: flex;\r\n  gap: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filter-input {\r\n  width: 150px;\r\n}\r\n\r\n.button-row {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.auto-search-text {\r\n  font-size: 14px;\r\n  color: #333;\r\n  margin-right: 10px;\r\n}\r\n\r\n.search-buttons {\r\n  display: flex;\r\n  gap: 3px;\r\n}\r\n\r\n.pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.el-dropdown-link {\r\n  cursor: pointer;\r\n  color: #409EFF;\r\n  font-weight: bold;\r\n}\r\n\r\n.edit-dialog .el-form-item {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.edit-dialog .el-dialog {\r\n  box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.dialog-footer {\r\n  text-align: right;\r\n}\r\n</style>\r\n\r\n"],"mappings":"AAgGA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;M